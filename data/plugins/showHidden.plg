(function() {
    var hidden = (document.querySelectorAll('[style="display:none"]'));
    for (let a = 0; a < hidden.length; a++) {
        hidden[a].setAttribute("style", "margin:0;padding:0;border:none");
        console.log(hidden[a]);
    }
})();